DATA:
  lista #A0
  n 5
  res 0
  aux 0
CODE:
siguiente:
  MOV B,(lista)
  MOV A,(B)
  INC B
  MOV B,(B)
  CMP A,B
  JGE comparar
  MOV (aux),A
  MOV A,B
  MOV B,(aux)
comparar:
  SUB A,B
  CMP A,(res)
  JLE revisar
  MOV (res),A
revisar:
  MOV A,(n)
  SUB A,1
  MOV (n),A
  INC (lista)
  CMP A,1
  JGT siguiente
